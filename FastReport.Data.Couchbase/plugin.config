<?xml version="1.0" encoding="utf-8"?>
	<config versions="net45;netstandard2.0">
		<description>Represents a connection to Couchbase server for FastReport.Net.</description>
		<plugins>
			<plugin Name="FastReport.Data.Json" />
		</plugins>
		<packages>
			<package Name="CouchbaseNetClient" Version="2.5.9"/>
		</packages>
		<libs>
			<lib Name="CouchbaseNetClient.2.5.9\lib\{framework}\Couchbase.NetClient.dll" />
			
			<libsFolder for="netstandard2.0" Name="NETStandard.Library.2.0.3\build\netstandard2.0\ref" />
		</libs>
		<refs>
			<ref Name="Couchbase.NetClient.dll" />
			
			<ref for="netstandard2.0" Name="netstandard.dll" />
		</refs>
	</config>